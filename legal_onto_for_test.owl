<owl:Class rdf:about="#Joint_Circulars">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Legal_Normative_Document"/>
        <owl:Restriction>
          <owl:onClass>
            <owl:Class>
              <owl:unionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#State_Auditor"/>
                <rdf:Description rdf:about="#Peoples_Supreme_Court"/>
                <rdf:Description rdf:about="#Supreme_Peoples_Court"/>
                <rdf:Description rdf:about="#Ministry_and_Ministerial_Level_Agencies"/>
              </owl:unionOf>
            </owl:Class>
          </owl:onClass>
          <owl:onProperty rdf:resource="#canBePromulgatedBy"/>
          <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
        </owl:Restriction>
        <owl:Restriction>
          <owl:someValuesFrom>
            <owl:Class>
              <owl:unionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#State_Auditor"/>
                <rdf:Description rdf:about="#Peoples_Supreme_Court"/>
                <rdf:Description rdf:about="#Supreme_Peoples_Court"/>
              </owl:unionOf>
            </owl:Class>
          </owl:someValuesFrom>
          <owl:onProperty rdf:resource="#canBePromulgatedBy"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Legal_Normative_Document"/>
</owl:Class>


